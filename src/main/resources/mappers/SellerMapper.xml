<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.paul9537.test.lesson04.dao.SellerDAO">
	
	<insert id="insertSeller" parameterType="com.paul9537.test.lesson04.model.Seller">
		INSERT INTO `seller`
		(
			  `id`
			, `nickname`
			, `profileImageUrl`
			, `temperature`
			, `createdAt`
			, `updatedAt`
		) VALUE (
			#{nickname}
			, #{profileImageUrl}
			, #{temperature}
		)
	</insert>
	
	<select id="getSellerInfo" resultType="com.paul9537.test.lesson04.model.Seller">
		SELECT
			`id`
			, `nickname`
			, `profileImageUrl`
			, `temperature`
			, `createdAt`
			, `updatedAt`
		FROM
			`seller`
		ORDER BY `id` DESC
		LIMIT 1
	</select>
	

</mapper>